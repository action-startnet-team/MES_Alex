<div class="mac-item" data-mac_code="">
    <div class="mac-item-head">
        <span class="mac_name"></span>
        <span class="mo-div">
            <span class="mo_code"></span>
        </span>
        <br />
        <div class="mx-wight">
            <span class="pro_name"></span>
            <span class="wrk_code"></span>
        </div>
    </div>
    <div class="status-div">
        <span data-status_code="" class="status status-color-default">關機</span>
    </div>
    <div class="oee-container">
        <div class="oee-item right-label" data-name="">
            <span class="yield-label item-label">產量</span>
            <span class="yield-value item-value"></span>
        </div>
        <div class="oee-item left-label" data-name="">
            <span class="utilization-label item-label">良品率</span>
            <span class="utilization-value item-value"></span>
        </div>
        <div class="oee-item right-label" data-name="">
            <span class="stop_time-label item-label">良品</span>
            <span class="stop_time-value item-value"></span>
        </div>
        <div class="oee-item left-label" data-name="">
            <span class="work_time-label item-label">達成率</span>
            <span class="work_time-value item-value"></span>
        </div>
        <div class="oee-item right-label" data-name="" style="height: 40px;">
            <span class="work_time-label item-label">不良品</span>
            <span class="work_time-value item-value"></span>
        </div>

        <div class="oee-item left-label" data-name="" style="height: 40px;">
            <span class="work_time-label item-label">人員</span>
            <span class="work_time-value item-value"></span>
        </div>

        <div class="oee-item full-label" data-name="">
            <span class="work_time-label item-label">日期</span>
            <span class="work_time-value item-value"></span>
        </div>

        @*<div class="oee-item full-label" data-name="">
                <span class="work_time-label item-label">效率</span>
                <span class="work_time-value item-value"></span>
            </div>*@

    </div>
    <div class="oee-container oee__container--info">
        <div class="oee__done oee__done__item">
            @*<div class="title title-h2"><span class="done">今日等待進行</span></div>*@
            <table id="oee_Table" class="table table-striped table-dark" style="margin-left: 25px;width: 440px;"></table>
        </div>
        @*<div class="oee__wait oee__wait__item">
            <div class="title title-h2"><span class="done">今日已經完成</span></div>
            <table id="oee_Table_2" class="table table-striped table-dark"></table>
        </div>*@
    </div>
</div>
