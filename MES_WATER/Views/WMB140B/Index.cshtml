                function AjaxData(successFunc, oOptions) {
                    let defaultAjaxOption = {
                        method: defaultOptions.method,
                        url: defaultOptions.url,
                        data: defaultOptions.data,
                        beforeSend: function (xhr) {
                            // to adjust
                            clearAll()

                            // loading
